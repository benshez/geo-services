  "productName": "GeoService Desktop",
  "description": "GeoService Desktop Application",
  "private": true,
  "main": "main.js",
  "scripts": {
    "watch": "npm run watch:dev",
    "watch:dev": "webpack --watch --progress --profile",
    "build": "npm run build:dev",
    "build:dev": "webpack --progress --profile",
    "package:win32": "electron-packager . --overwrite --asar --platform=win32 --arch=ia32 --prune=true --out=release-builds",
    "package-win64": "electron-packager . --overwrite --asar --platform=win32 --arch=x64 --prune=true --out=release-builds",
    "package-mac": "electron-packager . --overwrite --asar --platform=darwin --arch=x64 --prune=true --out=release-builds",
    "package-linux": "electron-packager . --overwrite --asar --platform=linux --arch=x64 --prune=true --out=release-builds",
    "package-all": "npm run package -- --all",
    "electron": "electron .",
    "webpack-test": "webpack --config webpack.test.js --progress --profile",
    "test": "karma start",
    "postinstall": "npm link geoservice-shared"
  },
  "dependencies": {
    "@angular/common": "^4.4.1",
    "@angular/compiler": "^4.4.1",
