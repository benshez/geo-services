    "ngrx-store-freeze": "^0.2.0",
    "@ngx-translate/core": "^8.0.0",
    "@ngx-translate/http-loader": "^2.0.0",
    "angulartics2": "2.2.1",
    "copy-webpack-plugin": "^4.0.1",
    "electron": "^1.8.0",
    "hammerjs": "^2.0.8",
    "webpack-target-electron-renderer": "^0.4.0",
    "core-js": "^2.4.1",
    "rxjs": "^5.5.2",
    "zone.js": "^0.8.4"
  },
